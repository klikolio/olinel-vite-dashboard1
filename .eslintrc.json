{
	"extends": "plugin:prettier/recommended",
	"plugins": ["@typescript-eslint"],
	"parser": "@typescript-eslint/parser",
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "warn",
		"no-undef": "off",
		"no-new": "off"
	}
}
